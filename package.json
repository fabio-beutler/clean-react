  "author": "Fabio Beutler <dev.fabiobeutler@gmail.com>",
  "description": "A clean React project with Next.js, TailwindCSS, TypeScript, ESLint, Prettier, Vitest, Testing Library and more.",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
